#include<stdio.h>

int main()
{
    
    int arr[5] = { 1, 2, 4, 8, 16 };

   
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }
    printf("\n");

    
    arr[3] = 167;

    
    for (int i = 0; i < 5; i++) {
        printf("%d ", arr[i]);
    }

    return 0;
}
output;
1 2 4 8 16 
1 2 4 167 16 
